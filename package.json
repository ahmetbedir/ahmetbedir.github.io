  "dependencies": {
    "handlebars": "^4.7.8",
    "jsonresume-theme-even": "^0.26.1",
    "jsonresume-theme-modern": "file:./src/themes/modern",
    "jsonresume-theme-professional": "file:./src/themes/professional",
    "jsonresume-theme-short": "file:./src/themes/short",
