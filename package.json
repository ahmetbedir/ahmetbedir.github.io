{
  "name": "ahmetbedir.github.io",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "site": "resumed config/resume.json --theme jsonresume-theme-even",
    "short": "resumed config/resume.json --theme jsonresume-theme-short --output dist/resume-short.html",
    "modern": "resumed config/resume.json --theme jsonresume-theme-modern --output dist/resume-modern.html",
    "professional": "resumed render config/resume.json --theme jsonresume-theme-professional --output dist/resume-professional.html",
    "pdf": "resumed export config/resume.json --theme jsonresume-theme-modern --output dist/resume-modern.pdf",
    "even": "resumed render config/resume.json --theme jsonresume-theme-even --output dist/resume-even.html",
    "build": "node src/index.js",
    "create-theme": "node scripts/create-theme.js"
  },
